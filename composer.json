{
    "name": "jenssegers/oauth",
    "description": "Laravel OAuth library based on Lusitanian/PHPoAuthLib",
    "keywords": ["oauth", "laravel"],
    "homepage": "https://github.com/jenssegers/laravel-oauth",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        },
        {
            "name": "Hannes Van De Vreken",
            "homepage": "http://hannesv.be"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "4.*|5.*",
        "lusitanian/oauth": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "mockery/mockery": "^0.9",
        "satooshi/php-coveralls": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "Jenssegers\\OAuth\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
